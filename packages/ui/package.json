{
  "name": "@acme/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./button": {
      "types": "./src/components/button/index.ts",
      "import": "./src/components/button/index.ts"
    },
    "./card": {
      "types": "./src/components/card/index.ts",
      "import": "./src/components/card/index.ts"
    },
    "./code": {
      "types": "./src/components/code/index.ts",
      "import": "./src/components/code/index.ts"
    },
    "./typography": {
      "types": "./src/components/typography/index.ts",
      "import": "./src/components/typography/index.ts"
    },
    "./alert": {
      "types": "./src/components/alert/index.ts",
      "import": "./src/components/alert/index.ts"
    },
    "./accordion": {
      "types": "./src/components/accordion/index.ts",
      "import": "./src/components/accordion/index.ts"
    },
    "./badge": {
      "types": "./src/components/badge/index.ts",
      "import": "./src/components/badge/index.ts"
    },
    "./form": {
      "types": "./src/components/form/index.ts",
      "import": "./src/components/form/index.ts"
    },
    "./validation": {
      "types": "./src/validation/index.ts",
      "import": "./src/validation/index.ts"
    },
    "./lib/utils": {
      "types": "./src/lib/utils.ts",
      "import": "./src/lib/utils.ts"
    },
    "./lib/uswds-tokens": {
      "types": "./src/lib/uswds-tokens.ts",
      "import": "./src/lib/uswds-tokens.ts"
    },
    "./lib/uswds-utils": {
      "types": "./src/lib/uswds-utils.ts",
      "import": "./src/lib/uswds-utils.ts"
    },
    "./styles/*": "./src/styles/*",
    "./dist/styles.css": "./dist/styles.css",
    "./dist/styles.min.css": "./dist/styles.min.css"
  },
  "scripts": {
    "build": "storybook build && pnpm build:css",
    "build:css": "tsx scripts/build-css.ts",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "lint:all": "pnpm lint && pnpm typecheck",
    "lint:fix:all": "pnpm lint:fix && pnpm typecheck",
    "test": "concurrently -k -s first -n \"SB,TEST\" \"http-server storybook-static --port 6006 --silent\" \"wait-on tcp:127.0.0.1:6006 && test-storybook --ci --url http://127.0.0.1:6006\"",
    "test:unit": "vitest",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:watch": "test-storybook --watch",
    "dev": "storybook dev -p 6006 --no-open",
    "dev:open": "storybook dev -p 6006",
    "generate:component": "turbo gen react-component"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/tailwind-config": "workspace:*",
    "@acme/typescript-config": "workspace:*",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.23.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.1.0",
    "concurrently": "^9.2.0",
    "cssnano": "^7.0.6",
    "eslint": "^9.31.0",
    "eslint-plugin-storybook": "^0.11.0",
    "http-server": "^14.1.1",
    "jsdom": "^25.0.1",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "5.8.2",
    "vite": "^5.4.19",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@uswds/uswds": "^3.13.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17"
  }
}
